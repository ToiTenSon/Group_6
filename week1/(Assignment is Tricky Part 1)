// Định nghĩa lớp Number với một thuộc tính công khai i kiểu int
public class Number {
    public int i; // Thuộc tính i của lớp Number
}

// Định nghĩa lớp Assignment1 chứa phương thức main để thực thi chương trình
public class Assignment1 {
    public static void main(String[] args) {
        // Tạo đối tượng n1 của lớp Number
        Number n1 = new Number();
        // Tạo đối tượng n2 của lớp Number
        Number n2 = new Number();
        
        // Gán giá trị 2 cho thuộc tính i của đối tượng n1
        n1.i = 2;
        // Gán giá trị 5 cho thuộc tính i của đối tượng n2
        n2.i = 5;
        
        // Gán giá trị của thuộc tính i của n2 (5) cho thuộc tính i của n1
        n1.i = n2.i; // bây giờ n1.i có giá trị 5
        
        // Thay đổi giá trị của thuộc tính i của n2 thành 10
        n2.i = 10; // n2.i = 10, nhưng điều này không ảnh hưởng đến n1.i
        
        // Tại thời điểm này, giá trị của n1.i là 5 và giá trị của n2.i là 10
        // Giá trị của n1.i không thay đổi khi giá trị của n2.i thay đổi
    }
}
